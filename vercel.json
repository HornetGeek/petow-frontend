{
  "version": 2,
  "builds": [
    {
      "src": "petmatch-nextjs/package.json",
      "use": "@vercel/next"
    }
  ],
  "buildCommand": "npm install && npm run build",
  "outputDirectory": "petmatch-nextjs/.next",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "http://13.60.199.22:8000/api/$1"
    },
    {
      "src": "/admin/(.*)",
      "dest": "http://13.60.199.22:8000/admin/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "http://13.60.199.22:8000/media/$1"
    },
    {
      "src": "/static/(.*)",
      "dest": "http://13.60.199.22:8000/static/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "http://13.60.199.22:8000/api"
  }
} 